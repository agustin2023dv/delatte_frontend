{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".", 
    "paths": {
      "@shared/*": ["src/shared/*"],
      "@features/*": ["src/features/*"],
      "@hooks/*": ["src/shared/hooks/*"],
      "@ui/*": ["src/shared/components/ui/*"]
    }
  },
"include": ["env.d.ts", "src/**/*.tsx", "src/**/*.ts"]
}
